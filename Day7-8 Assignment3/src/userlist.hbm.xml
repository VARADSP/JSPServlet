<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<!-- Class Mapping of LoginBean -->
	<class name="com.uks.varad.struts.day5_6.assignment.bean.UserDataBean" table="struts_userlist">
		<id name="userId">
			<generator class="increment"></generator>
		</id>
		<property name="name"></property>
		<property name="category"></property>
		<property name="sex"></property>
		<property name="address"></property>
		<property name="emailId"></property>
		<property name="isDisabled"></property>
	</class>

</hibernate-mapping>